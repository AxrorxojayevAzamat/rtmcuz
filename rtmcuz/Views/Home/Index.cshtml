@section Styles {
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper/swiper-bundle.min.css" />
    <link rel="stylesheet" href="~/css/leaflet.css" />
}

<div class="text-center">
    @await Html.PartialAsync("Home/Banner")
    @await Html.PartialAsync("Home/OurServices")
    @await Html.PartialAsync("Home/InteractiveServices", ViewData["InteractiveServices"])
    @await Html.PartialAsync("Home/News")
    @await Html.PartialAsync("Home/NumbersAboutUs")
    @await Html.PartialAsync("Home/Questions")
    @await Html.PartialAsync("Home/Feedback")
    @await Html.PartialAsync("Home/Map")
</div>

@section Scripts {
    <script src="https://cdn.jsdelivr.net/npm/swiper/swiper-bundle.min.js"></script>
    <script src="~/js/swiper-custom.js"></script>
    <script src="~/js/leaflet.js"></script>
    <script src="~/js/leaflet-custom.js"></script>
}